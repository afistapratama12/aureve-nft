# WalletConnect Configuration
# Get your project ID from https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Alchemy API Keys
# Get your API keys from https://dashboard.alchemy.com
VITE_ALCHEMY_API_KEY_SEPOLIA=your_alchemy_sepolia_api_key
VITE_ALCHEMY_API_KEY_ARB_SEPOLIA=your_alchemy_arbitrum_sepolia_api_key

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Pinata Configuration (IPFS)
# Get your API key from https://app.pinata.cloud
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_GATEWAY_DOMAIN=your_gateway_domain

# Blockchain Configuration
# Private key for lazy minting signer (without 0x prefix)
SIGNER_PRIVATE_KEY=your_private_key_here

# Network RPCs
VITE_ETHEREUM_SEPOLIA_RPC=https://rpc.ankr.com/eth_sepolia
VITE_ARBITRUM_SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc
VITE_MONAD_TESTNET_RPC=https://testnet-rpc.monad.xyz

# Smart Contract Addresses (will be filled after deployment)
VITE_NFT_CONTRACT_ADDRESS_ETHEREUM_SEPOLIA=
VITE_NFT_CONTRACT_ADDRESS_ARBITRUM_SEPOLIA=
VITE_NFT_CONTRACT_ADDRESS_MONAD_TESTNET=

# LayerZero Bridge Addresses (will be filled after deployment)
VITE_BRIDGE_ADDRESS_SEPOLIA=
VITE_BRIDGE_ADDRESS_ARBITRUM=
VITE_BRIDGE_ADDRESS_MONAD=

# Platform Configuration
VITE_PLATFORM_FEE_PERCENTAGE=1
VITE_PLATFORM_WALLET_ADDRESS=your_platform_wallet_address

# Optional: Etherscan API Keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
